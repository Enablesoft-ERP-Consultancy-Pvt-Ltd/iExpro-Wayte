{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "iexpro-wayte",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://releases.myapp.com/{{target}}/{{arch}}/{{current_version}}"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDhFQTNBQjg0QThDQ0FEQjEKUldTeHJjeW9oS3VqanZKenJsS1ovdnk3cTlJekpVdFJkdzBqQVAraG1LUmgxUFdObjJTbkxZRDAK"
    },
    "windows": [
      {
        "title": "iExpro Wayte",
        "label": "iExproWayte",
        "center": true,
        "minWidth": 800,
        "minHeight": 600,
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": ["appimage", "msi", "nsis", "updater"],
      "identifier": "com.iexpro.wayte",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
